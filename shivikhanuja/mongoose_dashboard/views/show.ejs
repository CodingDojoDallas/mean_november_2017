<!DOCTYPE html>
<html lang="en">
<head>
    <title>Puppy Info</title>
</head>
<body>
    <% for (x in puppys) { %>
        <div>
            <label>Name: </label>
            <a href="/index/<%= puppys[x]._id %>"><%= puppys[x].name %></a>
        </div>
        <div>
            <label>About: </label>
            <%= puppys[x].about %>
        </div>
        <div>
            <label>Color: </label>
            <%= puppys[x].color %>
        </div>
        <div>
            <label>City: </label>
            <%= puppys[x].city %>
        </div>
        <div>
            <form action="/<%= puppys[x]._id %>/edit" method="get">
            <input type="submit" value="Edit">
            
            </form>
        </div>
    <% } %>
    <hr>
</body>
</html>